{
  "private": true,
  "name": "client",
  "version": "0.1.0",
  "scripts": {
    "build": "react-scripts build",
    "format": "prettier --write 'src/*{.js,css}' && prettier-package-json --write package.json",
    "start": "react-scripts start",
    "test": "CI=true run-s test:formatting test:lint test:react",
    "test:formatting": "prettier --check 'src/*.{js,css}'",
    "test:lint": "eslint src --ext js",
    "test:react": "react-scripts test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "axios": "^0.21.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "prettier-package-json": "2.5.0"
  },
  "engines": {
    "node": ">=14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
